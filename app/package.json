{
    "name": "congressional-youtube-dashboard",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "clean": "rm -rf ../docs congressional-tech",
        "static": "echo 'test at ----> http://localhost:8000/congressional-tech <----' && python -m http.server",
        "build": "npm run clean && distDir=congressional-tech prefix=/congressional-tech next build --turbopack",
        "export": "npm run build && cp -r congressional-tech ../docs && touch ../docs/.nojekyll && npm run static"
    },
    "dependencies": {
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.6",
        "@tanstack/react-query": "^5.85.6",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.541.0",
        "next": "15.5.0",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "recharts": "^2.15.4",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.5.0",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "tailwindcss": "^4",
        "tw-animate-css": "^1.3.7",
        "typescript": "^5"
    }
}
